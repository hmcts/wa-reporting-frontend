{
  "useCSRFProtection": true,
  "csrfCookieSecret": "csrf-cookie-secret",
  "appInsights": {
    "connectionString": false
  },
  "security": {
    "referrerPolicy": "same-origin",
    "hsts": {
      "maxAge": 31536000,
      "includeSubDomains": true,
      "preload": true
    }
  },
  "govukFrontend": {
    "rebrandEnabled": true
  },
  "analytics": {
    "cacheTtlSeconds": 900,
    "manageCaseBaseUrl": "https://manage-case.aat.platform.hmcts.net"
  },
  "logging": {
    "prismaQueryTimings": false
  },
  "auth": {
    "enabled": true
  },
  "services": {
    "idam": {
      "clientID": "wa_reporting_frontend",
      "clientSecret": "wa_reporting_frontend_secret",
      "scope": "openid profile roles",
      "url": {
        "public": "https://idam-web-public.aat.platform.hmcts.net",
        "api": "https://idam-api.aat.platform.hmcts.net",
        "wa": "http://localhost:3100"
      }
    }
  },
  "RBAC": {
    "access": "wa-reports-viewer"
  },
  "session": {
    "secret": "session-secret",
    "cookie": {
      "name": "wa-reporting-frontend-session"
    },
    "appCookie": {
      "name": "wa-reporting-frontend-app"
    },
    "redis": {
      "host": "",
      "port": 6379,
      "key": ""
    }
  },
  "database": {
    "tm": {
      "host": "localhost",
      "port": 5432,
      "user": "postgres",
      "password": "password",
      "db_name": "cft_task_db",
      "schema": "analytics",
      "options": ""
    },
    "crd": {
      "host": "localhost",
      "port": 5432,
      "user": "postgres",
      "password": "password",
      "db_name": "dbrdcaseworker",
      "schema": "rdstaffreport",
      "options": ""
    },
    "lrd": {
      "host": "localhost",
      "port": 5432,
      "user": "postgres",
      "password": "password",
      "db_name": "dbrdlocationref",
      "schema": "locrefdata",
      "options": ""
    }
  }
}
