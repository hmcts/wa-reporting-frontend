{
  "useCSRFProtection": true,
  "security": {
    "referrerPolicy": "same-origin",
    "hsts": {
      "maxAge": 31536000,
      "includeSubDomains": true,
      "preload": true
    }
  },
  "analytics": {
    "cacheTtlSeconds": 900,
    "manageCaseBaseUrl": "https://manage-case.aat.platform.hmcts.net",
    "filtersCookieName": "wa-reporting-analytics-filters",
    "filtersCookieMaxAgeDays": 365
  },
  "logging": {
    "prismaQueryTimings": {
      "enabled": false,
      "minDurationMs": 0,
      "slowQueryThresholdMs": 0,
      "includeQueryPreview": false,
      "queryPreviewMaxLength": 240
    }
  },
  "auth": {
    "enabled": true
  },
  "compression": {
    "enabled": false
  },
  "services": {
    "idam": {
      "clientID": "wa_reporting_frontend",
      "scope": "openid profile roles",
      "url": {
        "public": "https://hmcts-access.aat.platform.hmcts.net",
        "wa": "http://localhost:3100"
      }
    }
  },
  "RBAC": {
    "access": "wa-reports-viewer"
  },
  "session": {
    "cookie": {
      "name": "wa-reporting-frontend-session"
    },
    "appCookie": {
      "name": "wa-reporting-frontend-app"
    }
  },
  "database": {
    "tm": {
      "host": "localhost",
      "port": 5432,
      "db_name": "cft_task_db",
      "schema": "analytics",
      "options": ""
    },
    "crd": {
      "host": "localhost",
      "port": 5432,
      "db_name": "dbrdcaseworker",
      "schema": "rdstaffreport",
      "options": ""
    },
    "lrd": {
      "host": "localhost",
      "port": 5432,
      "db_name": "dbrdlocationref",
      "schema": "locrefdata",
      "options": ""
    }
  },
  "secrets": {
    "wa": {
      "app-insights-connection-string": false,
      "wa-reporting-redis-host": "",
      "wa-reporting-redis-port": 6379,
      "wa-reporting-redis-access-key": "",
      "wa-reporting-frontend-client-secret": "wa_reporting_frontend_secret",
      "session-secret": "session-secret",
      "csrf-cookie-secret": "csrf-cookie-secret",
      "tm-db-user": "postgres",
      "tm-db-password": "password",
      "crd-db-user": "postgres",
      "crd-db-password": "password",
      "lrd-db-user": "postgres",
      "lrd-db-password": "password"
    }
  }
}
