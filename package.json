{
  "name": "wa-reporting-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node dist/main/server.js",
    "start:dev": "nodemon --config nodemon.json",
    "lint": "stylelint **/*.scss -v -q && eslint . --ext .js,.ts && prettier --check .",
    "lint:fix": "prettier --write . .github && eslint --fix .",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js && yarn copy:static",
    "copy:static": "node scripts/copy-static.js",
    "build:server": "tsc -p tsconfig.json",
    "test": "npx if-env CI=true && exit 0 || yarn test:unit",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:routes": "jest -c jest.routes.config.js",
    "test:mutation": "stryker run",
    "test:mutation:ci": "stryker run --reporters clear-text,progress --fileLogLevel warn",
    "test:a11y": "NODE_TLS_REJECT_UNAUTHORIZED=0 AUTH_ENABLED=false LOG_LEVEL=debug PLAYWRIGHT_HTML_OUTPUT=playwright-report/a11y PLAYWRIGHT_OUTPUT_DIR=test-results/a11y yarn playwright test -c playwright.a11y.config.mjs --grep @a11y",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 PLAYWRIGHT_HTML_OUTPUT=playwright-report/smoke PLAYWRIGHT_OUTPUT_DIR=test-results/smoke yarn playwright test --grep @smoke --project=chromium",
    "test:functional": "NODE_TLS_REJECT_UNAUTHORIZED=0 PLAYWRIGHT_HTML_OUTPUT=playwright-report/functional PLAYWRIGHT_OUTPUT_DIR=test-results/functional yarn playwright test --grep @functional --project=chromium --project=firefox --project=webkit",
    "test:ui": "yarn playwright test --ui",
    "setup:edge": "PLAYWRIGHT_BROWSERS_PATH=0 yarn playwright install msedge",
    "cichecks": "yarn install && yarn build && yarn rebuild puppeteer && yarn lint && yarn test && yarn test:routes && yarn test:a11y",
    "env:aat": "node scripts/get-env.js aat",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry": "^1.15.1",
    "@hmcts/info-provider": "^1.4.0",
    "@hmcts/nodejs-healthcheck": "^1.8.6",
    "@hmcts/properties-volume": "^1.3.1",
    "@ministryofjustice/frontend": "^8.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation": "^0.212.0",
    "@opentelemetry/instrumentation-express": "^0.60.0",
    "@opentelemetry/instrumentation-winston": "^0.56.0",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "axios": "^1.13.5",
    "body-parser": "^2.2.2",
    "compression": "^1.8.1",
    "config": "^4.3.0",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "csrf-sync": "^4.2.1",
    "express": "^5.2.1",
    "express-nunjucks": "^3.1.2",
    "express-openid-connect": "^2.19.4",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.19.0",
    "glob": "^13.0.5",
    "govuk-frontend": "^6.0.0",
    "helmet": "^8.1.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "node-cache": "^5.1.2",
    "nunjucks": "^3.2.4",
    "pg": "^8.18.0",
    "plotly.js-basic-dist-min": "^3.3.1",
    "redis": "^5.11.0",
    "require-directory": "^2.1.1",
    "serve-favicon": "^2.5.1",
    "session-file-store": "^1.5.0",
    "superagent": "^10.3.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/eslint-parser": "^7.28.6",
    "@babel/preset-env": "^7.29.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@hmcts/playwright-common": "^1.1.2",
    "@playwright/test": "^1.58.2",
    "@stryker-mutator/core": "^9.5.1",
    "@stryker-mutator/jest-runner": "^9.5.1",
    "@stryker-mutator/typescript-checker": "^9.5.1",
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/es6-promisify": "^6.0.4",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
