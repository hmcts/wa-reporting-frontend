    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "webpack": "5.104.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-middleware": "7.4.5",
    "webpack-node-externals": "3.0.0"
  },
  "resolutions": {
    "diff": "^8.0.3",
    "hono": "4.11.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{js,ts}": "eslint --fix"
  },
  "packageManager": "yarn@4.12.0"
}
