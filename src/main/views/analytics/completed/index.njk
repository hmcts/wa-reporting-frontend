{% extends "analytics/layout.njk" %}
{% from "govuk/components/summary-list/macro.njk" import govukSummaryList %}
{% from "macros/analytics-table.njk" import analyticsTable %}
{% from "govuk/components/tabs/macro.njk" import govukTabs %}
{% from "macros/csv-download.njk" import csvDownloadButton %}

{% block analyticsContent %}
  <div class="govuk-width-container govuk-width-container--full">
    <h1 class="govuk-heading-xl">Completed tasks</h1>
    <p class="govuk-body-l">Details of completed tasks</p>

    <div data-section="shared-filters">
      {% include "analytics/completed/completed-filters.njk" %}
    </div>
    <div data-section="completed-summary" data-ajax-initial="true">
      {% include "analytics/completed/partials/completed-summary.njk" %}
    </div>
    <div data-section="completed-timeline" data-ajax-initial="true">
      {% include "analytics/completed/partials/completed-timeline.njk" %}
    </div>
    <div data-section="completed-by-name" data-ajax-initial="true">
      {% include "analytics/completed/partials/completed-by-name.njk" %}
    </div>
    <div data-section="completed-processing-handling-time" data-ajax-initial="true">
      {% include "analytics/completed/partials/processing-handling-time.njk" %}
    </div>
    <div data-section="completed-task-audit" data-ajax-initial="true">
      {% include "analytics/completed/partials/task-audit.njk" %}
    </div>
    <div data-section="completed-by-region-location" data-ajax-initial="true">
      {% include "analytics/completed/partials/completed-by-region-location.njk" %}
    </div>
  </div>
{% endblock %}
